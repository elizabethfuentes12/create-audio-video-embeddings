# AWS Region
REACT_APP_REGION=us-east-1

# Cognito User Pool
REACT_APP_USER_POOL_ID=us-east-1_xxxxxxxxx
REACT_APP_USER_POOL_WEB_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
REACT_APP_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# S3 Bucket for Videos
REACT_APP_S3_BUCKET=your-video-bucket-name

# API Gateway Endpoint
REACT_APP_API_ENDPOINT=https://xxxxxxxxxx.execute-api.us-east-1.amazonaws.com/prod

# Step Functions ARN
REACT_APP_STEP_FUNCTION_ARN=arn:aws:states:us-east-1:123456789012:stateMachine:VideoProcessingWorkflow